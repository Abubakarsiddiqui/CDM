{
    "$schema": "../schema.cdm.json",
    "schemaVersion": "0.6.0",
    "imports": [
        {
            "uri": "/foundations.cdm.json"
        }
    ],
    "definitions": [
        {
            "traitName": "sampleEntityTrait",
            "extendsTrait": "is",
            "hasParameters": [
                {
                    "name": "P1",
                    "dataType": "string",
                    "defaultValue": "P1Def"
                },
                {
                    "name": "P2",
                    "dataType": "entity"
                }
            ]
        },
        {
            "entityName": "NamedItems",
            "extendsEntity": "CdmObject",
            "exhibitsTraits": [
                {
                    "traitReference": "sampleEntityTrait",
                    "arguments": [
                        {
                            "name": "P1",
                            "value": "P1FromBase"
                        },
                        {
                            "name": "P2",
                            "value": {
                                "entityReference": {
                                    "entityShape": "sampleList",
                                    "constantValues": [
                                        [
                                            "0",
                                            "Item zero"
                                        ],
                                        [
                                            "1",
                                            "Item One"
                                        ],
                                        [
                                            "2",
                                            "Item Two"
                                        ]
                                    ]
                                }
                            }
                        }
                    ]
                }
            ],
            "hasAttributes": [
                {
                    "name": "itemName",
                    "relationship": "namedBy",
                    "dataType": "name"
                },
                {
                    "name": "lastChecked",
                    "relationship": "hasA",
                    "dataType": "dateTime"
                },
                {
                    "name": "legacyGoo",
                    "relationship": "hasA",
                    "dataType": "string"
                },
                {
                    "name": "sequence",
                    "relationship": "orderedBy",
                    "dataType": "integer"
                },
                {
                    "name": "itemId",
                    "relationship": "identifiedBy",
                    "dataType": "entityId"
                }
            ]
        },
        {
            "entityName": "sampleList",
            "extendsEntity": "reference",
            "hasAttributes": [
                {
                    "name": "listItemNumber",
                    "relationship": "identifiedBy",
                    "dataType": "integer"
                },
                {
                    "name": "description",
                    "relationship": "hasA",
                    "dataType": "string"
                }
            ]
        }
    ]
}